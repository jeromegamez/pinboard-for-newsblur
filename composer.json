{
    "name": "jeromegamez/pinboard-for-newsblur",
    "description": "A proxy for pinboard.in feeds that fixes the keywords to be displayed correctly in NewsBlur",
    "license": "ISC",
    "type": "project",
    "authors": [
        {
            "name": "Jérôme Gamez",
            "email": "jerome@gamez.name"
        }
    ],
    "require": {
        "php": "~8.2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.34.1",
        "symfony/var-dumper": "^6.3.5"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": false
        }
    },
    "scripts": {
        "cs-fix": "vendor/bin/php-cs-fixer fix --diff --verbose"
    }
}
